cap-iacs-base:detailsOfExecutableTestCasesForConformanceReport
        a           owl:ObjectProperty ;
        rdfs:range  cap-iacs-base:ConformanceTestReport .

cap-iacs-base:AbstractTestCase
        a                owl:Class ;
        rdfs:label       "Abstract test case"@en ;
        rdfs:subClassOf  gsp:Feature ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   cap-iacs-base:testMethod
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   cap-iacs-base:detailsOfAbstractTestCasesForConformanceReport
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:minCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      cap-iacs-base:detailsOfAbstractTestCaseForExecutableTestCases
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:maxCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      cap-iacs-base:testingType
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:minCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      cap-iacs-base:testPurpose
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   cap-iacs-base:testType
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   cap-iacs-base:testName
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:minCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      cap-iacs-base:reference
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:maxCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      cap-iacs-base:falsificationTest
                         ] ;
        skos:definition  "--Name--\r\nAbstract test case\r\n\r\n--Definition--\r\nGeneralized test for a particular requirement [ISO19105:2000].\r\n\r\n--Description--\r\nAn abstract test case is a formal basis for deriving executable test cases. One or more test purposes are encapsulated in an abstract test case. An abstract test case is independent of both the implementation and the values. It should be complete in the sense that it is sufficient to assign a test verdict unambiguously to each potentially observable test outcome (i.e. sequence of test events)."@en .

cap-iacs-base:sunday  a  owl:DatatypeProperty ;
        rdfs:label       "Sunday"@en ;
        rdfs:range       xsd:boolean ;
        skos:definition  "-- Definition --\r\nSunday.\r\n-- Description --\r\nItem description."@en .

cap-iacs-base:everyWeekday
        a                owl:DatatypeProperty ;
        rdfs:label       "Every weekday"@en ;
        rdfs:range       xsd:boolean ;
        skos:definition  "-- Definition --\r\nEvery weekday.\r\n-- Description --\r\nItem description."@en .

cap-iacs-base:requirementClass
        a                owl:ObjectProperty ;
        rdfs:label       "Conformance requirement class"@en ;
        rdfs:range       cap-iacs-base:ConformanceRequirementClassValue ;
        skos:definition  "--Name--\r\nRequirement class\r\n\r\n--Definition--\r\nClassification of requirements based on specific criteria.\r\n\r\n--Description--\r\nThe criteria may include functional requirements (what the system must be able to do) and generic non-functional requirements (performance, traceability, scalability, etc.)"@en .

cap-iacs-base:friday  a  owl:DatatypeProperty ;
        rdfs:label       "Friday"@en ;
        rdfs:range       xsd:boolean ;
        skos:definition  "-- Definition --\r\nFriday.\r\n-- Description --\r\nItem description."@en .

cap-iacs-base:detailsOfANaturalPersonAsFarmer
        a           owl:ObjectProperty ;
        rdfs:range  cap-iacs-beneficiery:Farmer .

cap-iacs-base:SpecieCodeValue
        a                owl:Class ;
        rdfs:label       "List of vegetable and agricultural plant species"@en ;
        rdfs:subClassOf  skos:Concept ;
        skos:definition  "-- Definition --\r\nList of vegetable and agricultural plant species (aggregate of a varieties). \r\n-- Description --\r\nA culture of any of the different genera defined in the botanical classification of crops. In this case code list consist of binomial names of species from the taxonomy of Linnaeus (classification for plants).\r\nNOTE. Common catalogue of varieties of agricultural plant species. Fifth supplement to the 29th complete edition (2011/C 168 A/01)\r\nList of agricultural species -A\r\nI. Beet\r\nII. Fodder plants\r\nIII. Oil and fibre plants    \r\nIV. Cereals\r\nV. Potatoes \r\nNOTE. Common catalogue of varieties of vegetable species. 32nd complete edition (2013/C 312 A/01)\r\nList of vegetable species - H"@en .

cap-iacs-base:beginLifespanVersion
        a                owl:DatatypeProperty ;
        rdfs:label       "Begin lifespan version"@en ;
        rdfs:range       xsd:dateTime ;
        skos:definition  "-- Name--\r\nBegin life span version\r\n\r\n--Definition--\r\nSet of properties of an object/feature that describes the temporal characteristics of a version or the changes between versions. [Adapted from INSPIRE Generic Conceptual Model]\r\n\r\n-- Description --\r\nDate and time at which this version of the feature was inserted or changed in the dataset."@en .

cap-iacs-base:ixit  a    owl:ObjectProperty ;
        rdfs:label       "Implementation eXtra Information for testing provided by client"@en ;
        rdfs:range       cap-iacs-base:SupplementaryDocument ;
        skos:definition  "--Name--\r\nIXIT\r\n\r\n--Definition--\r\nImplementation eXtra Information for Testing provided by client.\r\n\r\n--Description--\r\nStatement containing all of the information related to the IUT and its corresponding SUT which will enable the testing laboratory to run an appropriate test suite against that IUT. Implementation eXtra Information for Testing provided by client to testing laboratory."@en .

cap-iacs-base:optionsWhichHaveBeenImplemented
        a           owl:ObjectProperty ;
        rdfs:range  cap-iacs-base:ConformanceRequirement .

cap-iacs-base:nonStructuredEvidenceCitation
        a                owl:ObjectProperty ;
        rdfs:label       "Non-structured evidence - citation"@en ;
        rdfs:range       cap-iacs-base:SupplementaryDocumentCitation ;
        skos:definition  "-- Definition --\r\nAny record that describes the cross-compliance rule.\r\n-- Description --\r\nURL."@en .

cap-iacs-base:resultMts
        a                owl:ObjectProperty ;
        rdfs:label       "The result of model test suite"@en ;
        rdfs:range       cap-iacs-base:ConformanceStatementValue ;
        skos:definition  "--Name--\r\nMTS result\r\n\r\n-- Definition --\r\nConformance verdict on the result of the MTS.\r\n\r\n-- Description --\r\nThe MTS of the eligibility profile has to be run against the current LCM every time when\r\n- a change in the application schema of the eligibility profile has been made (because of the change of MS/Region approach),\r\n- a change in the LCM has been made (because of the change in the EU regulation)."@en .

cap-iacs-base:NotificationGroupValue
        a                owl:Class ;
        rdfs:label       "List of notification groups."@en ;
        rdfs:subClassOf  skos:Concept ;
        skos:definition  "-- Definition --\r\nList of notification groups.\r\n-- Description --\r\nList of notification groups prescribed in legal documents.\r\nNOTE. Not finite number of notification groups. List may be extended by MS."@en .

cap-iacs-base:recurrence
        a                owl:ObjectProperty ;
        rdfs:label       "Recurrence"@en ;
        rdfs:range       cap-iacs-base:RecurenceValue ;
        skos:definition  "-- Definition --\r\nRecurrence.\r\n-- Description --\r\nRecurrence of notification. \r\nNOTE. Only one recurrenceValue may be used for one notification pattern. After particular reccurenceValue is selected - respective notification pattern (daily, weekly, monthly or yearly) must be defined."@en .

cap-iacs-base:WeekdayValue
        a                owl:Class ;
        rdfs:label       "List of weekday values"@en ;
        rdfs:subClassOf  skos:Concept ;
        owl:oneOf        ( <http://lpis.ec.europa.eu/registry/applicationschema/cap-iacs-base/code#WeekdayValue/friday> <http://lpis.ec.europa.eu/registry/applicationschema/cap-iacs-base/code#WeekdayValue/monday> <http://lpis.ec.europa.eu/registry/applicationschema/cap-iacs-base/code#WeekdayValue/saturday> <http://lpis.ec.europa.eu/registry/applicationschema/cap-iacs-base/code#WeekdayValue/sunday> <http://lpis.ec.europa.eu/registry/applicationschema/cap-iacs-base/code#WeekdayValue/thursday> <http://lpis.ec.europa.eu/registry/applicationschema/cap-iacs-base/code#WeekdayValue/tuesday> <http://lpis.ec.europa.eu/registry/applicationschema/cap-iacs-base/code#WeekdayValue/wednesday> ) ;
        skos:definition  "-- Definition --\r\nList of possible weekdays.\r\n-- Description --\r\nList of possible weekdays."@en .

cap-iacs-base:ExecutableTestCase
        a                owl:Class ;
        rdfs:label       "Executable test case"@en ;
        rdfs:subClassOf  gsp:Feature ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
