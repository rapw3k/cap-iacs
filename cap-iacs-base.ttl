        a                owl:ObjectProperty ;
        rdfs:label       "Notification group"@en ;
        rdfs:range       cap-iacs-base:NotificationGroupValue ;
        skos:definition  "-- Definition --\r\nNotification group.\r\n-- Description --\r\nNotification group to which notification pattern belongs."@en .

cap-iacs-base:CommunityFarmStructureSurvey
        a                owl:Class ;
        rdfs:label       "Community farm structure survey, land part only"@en ;
        rdfs:subClassOf  gsp:Feature ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   cap-iacs-base:farmStructureSurveyNumber
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   cap-iacs-base:cropItemCode
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:maxCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      cap-iacs-base:varietalOrSpecieInformationForAgriculturalParcelWithinFarmerDeclaration
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   cap-iacs-base:varietalName
                         ] ;
        skos:definition  "-- Definition --\r\nCommunity farm structure survey, land part only.\r\n-- Description --\r\nCommunity farm structure survey. Scope - LAND part only (Commision Regulation (EC) No 1200/2009 of 30 November 2009)\r\nMS shall create record set of this table themselves based on MS specific plant specie variety properties and within the scope of EU and national regulations. This table is dedicated for grouping and description of particular specie/variety.\r\nNOTE. This table may serve as parameter table. It is suggested to prepare record set for this table before declaration procedure starts (varietal names are MS specific). This record set then can be used to pre fill farmers application."@en .

