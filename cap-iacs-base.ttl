        skos:definition  "-- Definition --\r\nThe weekday.\r\n-- Description --\r\nNOTE. If option a are used, option b shall not be used."@en .

cap-iacs-base:ConformanceRequirement
        a                owl:Class ;
        rdfs:label       "Conformance requirement"@en ;
        rdfs:subClassOf  gsp:Feature ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   cap-iacs-base:requirementStatement
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   cap-iacs-base:requirementClass
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   cap-iacs-base:requirementDescription
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:minCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      cap-iacs-base:conformanceRequirementsForAbstractTestSuites
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   cap-iacs-base:detailsOfConformanceRequirementsForIcs
                         ] ;
        skos:definition  "--Name--\r\nConformance requirement\r\n\r\n-- Definition --\r\nRequirements to be fulfilled by the SUT in order to claim conformance to a specific conformance clause.\r\n\r\n-- Description --\r\nConformance requirement. The conformance requirements are stated in the conformance clause of each standard."@en .

cap-iacs-base:NotificationTemplate
        a                owl:Class ;
        rdfs:label       "Notification template"@en ;
        rdfs:subClassOf  [ a                owl:Restriction ;
