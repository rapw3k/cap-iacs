                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:maxCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      cap-iacs-base:aOnDay
                         ] ;
        skos:definition  "-- Definition --\r\nNotification pattern monthly.\r\n-- Description --\r\nNotification pattern with monthly notification properties."@en .

cap-iacs-base:farmStructureSurveyNumber
        a                owl:ObjectProperty ;
        rdfs:label       "Farm structure survey number"@en ;
        rdfs:range       cap-iacs-base:FarmStructureSurveyNumberValue ;
        skos:definition  "-- Definition --\r\nFarm structure survey number.\r\n-- Description --\r\n\"agregate code\" are mapped with farmStructureSurveyNumber.\r\nScope - LAND part only. Commision Regulation (EC) No 1200/2009 of 30 November 2009 implementing Regulation (EC) No 1166/2008 of the European Parliament and of the Council on farm structure surveys and the survey on agricultural production methods, as regards livestock unit coefficients and definitions of the characteristics (Text with EEA relevance)."@en .

cap-iacs-base:ConformanceLevelValue
        a                owl:Class ;
        rdfs:label       "List of conformance level values"@en ;
        rdfs:subClassOf  skos:Concept ;
        skos:definition  "--Name--\r\nConformance level value\r\n\r\n-- Definition --\r\nList of conformance level values.\r\n\r\n-- Description --\r\nList of conformance level values. Any product claiming conformance with one of these levels shall pass all requirements described in the corresponding abstract test suite.\r\n\r\nNOTE. Requirements of a higher level contain all requirements of a lower level."@en .

cap-iacs-base:OrdinalNumberValue
