  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "At least 3 crops with main crop less than 75%\r\n\r\nAt least three crops, the main crop covering less than the 75 % threshold"
  } ],
  "http://www.w3.org/2004/02/skos/core#inScheme" : [ {
