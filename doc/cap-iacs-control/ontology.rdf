    <owl:ObjectProperty rdf:about="http://lpis.ec.europa.eu/registry/applicationschema/cap-iacs-control#eligibilityCriteriaFulfilled">
        <rdfs:range rdf:resource="http://lpis.ec.europa.eu/registry/applicationschema/cap-iacs-base#ConformanceStatementValue"/>
        <rdfs:label xml:lang="en">Final entitlement area determined after all administrative checks</rdfs:label>
        <skos:definition xml:lang="en">-- Definition --
Final entitlement area determined after all administrative checks.
-- Description --
If AdministrativeCheck.determinedEntitlementArea&lt;&gt;Application.declaredEntitlementArea Then
AdministrativeCheck.determinedEntitlementArea = AdministrativeCheck.determinedEntitlementArea Else
AdministrativeCheck.determinedEntitlementArea = Application.declaredEntitlementArea
