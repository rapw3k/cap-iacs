<http://lpis.ec.europa.eu/registry/applicationschema/cap-iacs-aid>
        a                owl:Ontology ;
        rdfs:label       "Aid applications and payment claims"@en ;
        owl:imports      <https://github.com/HajoRijgersberg/OM/raw/master/om-2.0.rdf> , 
        								 <https://raw.githubusercontent.com/rapw3k/cap-iacs/main/cap-iacs-control.ttl> , 
        								 <http://www.opengis.net/ont/geosparql> , 
        								 <https://raw.githubusercontent.com/rapw3k/cap-iacs/main/cap-iacs-beneficiery.ttl> , 
        								 <https://raw.githubusercontent.com/rapw3k/cap-iacs/main/cap-iacs-parcel.ttl> , 
        								 <https://raw.githubusercontent.com/rapw3k/cap-iacs/main/cap-iacs-base.ttl> ;
        owl:versionIRI   "http://lpis.ec.europa.eu/registry/applicationschema/cap-iacs-aid/3.0" ;
        owl:versionInfo  "3.0" ;
        skos:definition  "-- Name --\r\nAid applications and payment claims\r\n\r\n-- Definition --\r\nThis is a placeholder.\r\n\r\n-- Description --\r\nAid applications and payment claims system application schema.The contents of an «ApplicationSchema» package exported as a GML Application Schema file."@en ;
        dct:creator [ schema:affiliation [ foaf:name "PSNC" ] ;
                      rdfs:seeAlso <http://orcid.org/0000-0003-4289-4922> ;
                      foaf:name "Raul Palma"
                    ] ;
        dct:title "JRC IACS - Aid Applications and Payment Claims Ontology"@en ;
        dct:description "Ontology of the aid applications and payment claims used in IACS, generated from the transformation of the corresponding JRC application schema"@en ;
        dct:source "JRC IACS application schema. https://doi.org/10.1016/j.landusepol.2016.05.023"@en ;
        dct:rights "This ontology is distributed under Creative Commons Attribution 4.0 License - http://creativecommons.org/licenses/by/4.0"@en .

cap-iacs-aid:farmersMeasurementId
        a                owl:DatatypeProperty ;
        rdfs:label       "Unique thematic ID of farmers survey"@en ;
        rdfs:range       xsd:string ;
        skos:definition  "-- Definition --\r\nUnique thematic ID of farmer's measurement within a survey.\r\n-- Description --\r\nThematic identifier at national level, generally the full national code of the farmer's  measurement within a survey.\r\nNOTE. Object (real world phenomena) description. It may be the code of object measured."@en .

cap-iacs-aid:agriculturalPracticeRequirementType
        a                owl:ObjectProperty ;
        rdfs:label       "Type of agricultural practice"@en ;
        rdfs:range       cap-iacs-aid:AgriculturalPracticeRequirementValue ;
        skos:definition  "Agricultural practice requirement type\r\n\r\nThis property describes the agricultural practices required for minimum activity (Art. 4) and greening, or for exemption from greening according to  Art. (44) and 46 of R (EU) 1307/2013."@en .

cap-iacs-aid:estimatedAccuracy
        a                owl:DatatypeProperty ;
        rdfs:label       "Estimated absolute positional accuracy "@en ;
        rdfs:range       xsd:double ;
        skos:definition  "-- Definition --\r\nEstimated absolute positional accuracy.\r\n-- Description --\r\nEstimated absolute positional accuracy of the object measured in the used coordinate reference system. Absolute positional accuracy is the mean value of the positional uncertainties for a set of positions, where the positional uncertainties are the distance between a measured position and what is considered as the corresponding true position. This mean value may come from quality measures on a homogeneous population of objects measured or from an estimation based on the knowledge of the production processes and of their accuracy."@en .

cap-iacs-aid:GeometrySourceValue
        a                owl:Class ;
        rdfs:label       "List of geometry sources"@en ;
        rdfs:subClassOf  skos:Concept ;
        skos:definition  "-- Definition --\r\nList of geometry sources.\r\n-- Description --\r\nList of geometry sources."@en .

