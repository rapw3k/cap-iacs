cap-iacs-aid:CropGroupAreaRelatedAidSchema
        a                owl:Class ;
        rdfs:label       "Payment crop group area related aid schema"@en ;
        rdfs:subClassOf  gsp:Feature ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   cap-iacs-aid:declaredArea
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   cap-iacs-aid:cropGruopTypeAreaRelatedAidSchema
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   cap-iacs-aid:detailsOfCropGroupAreaRelatedAidSchemaForPayment
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   cap-iacs-aid:cropGroupAreaRelatedAidSchemaDetailsInAidApplication
                         ] ;
        skos:definition  "-- Definition --\r\nPayment crop group area related aid schema.\r\n-- Description --\r\nCrop groups (in the context of IACS). Automatically defined and agregated by administration using application entries. This is placeholder for all payment schemas (except \"greening\", which have different place holder named CropGroupGreening) and area-related support measures (area-related support measures - in current version of the model not implemented yet)."@en .

cap-iacs-aid:agriculturalPracticeId
        a                owl:DatatypeProperty ;
        rdfs:label       "Unique thematic ID of agricultural practice"@en ;
